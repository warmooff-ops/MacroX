{
  "$schema": "../gen/schemas/capabilities.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:allow-write",
    "fs:allow-read"
  ]
}